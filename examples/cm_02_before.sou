# Moving a loop invariant
# BEFORE
  var one = 1
  var x = 3
  var y = 7
  var z = 0
  var i = 0
  var acc = 0
  var limit = 10
  read limit
  print z
  goto loop
loop:
  branch (i == limit) $continue $loop_body
$loop_body:
  # This assignment can be moved
  z <- (x + y)
  acc <- (acc + z)
  i <- (i + one)
