# Code motion without branch pruning
# AFTER
  var zero = 0
  var one = 1
  var two = 2
  var three = 3
  var x = 10
  var z = 0
  read z
  branch (z == zero) $l1 $l2
$l1:
  x <- (x + one)
  goto end
$l2:
  x <- (x + two)
  goto end
end:
  # This statement was moved
  x <- (x + three)
  print x
  stop 0
